    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "express-session-sequelize": "^1.1.0",
    "gridlex": "^2.2.4",
    "lodash": "^4.17.11",
    "moment": "^2.17.1",
    "morgan": "^1.9.1",
